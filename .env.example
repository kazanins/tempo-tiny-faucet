# Server Configuration
PORT=3000
NODE_ENV=development

# Tempo RPC Configuration
TEMPO_RPC_URL=https://rpc.testnet.tempo.xyz

# Private key for the service wallet (will receive from native faucet and distribute)
# Generate one using: cast wallet new
SERVICE_WALLET_PRIVATE_KEY=your_private_key_here

# Redis Configuration
# Option 1: Use REDIS_URL (recommended for Railway/production)
# REDIS_URL=redis://default:password@host:port
# Option 2: Use individual settings (for local development)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Rate Limiting (requests per address per time window)
RATE_LIMIT_WINDOW_MS=86400000
RATE_LIMIT_MAX_REQUESTS=3
